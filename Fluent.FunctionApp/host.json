{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "None",
      "Function": "Trace",
      "Host.Aggregator": "Critical",
      "Host.Results": "Error"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  }
}